    <questions>
        <question codigo="q1Automata" type="truefalse">
           <questiontext format="html">
             <text>Todo autómata finito <b>no determinista</b> puede convertirse en una autómata finito determinista equivalente</text>
           </questiontext>
           <answer codigo="a1">
                <text>verdadero</text>
            </answer>
            <answer codigo="a2">
                <text>
                    <b>falso </b>
                </text>
            </answer>
        </question>
        <question codigo="q2Automata" type="truefalse">
           <questiontext format="html">
               <text>Un autómata finito determinista <b>no</b> puede reconocer la <b>palabra vacía</b></text>
           </questiontext>
           <answer codigo="a1">
                <text>verdadero</text>
            </answer>
            <answer codigo="a2">
                <text>falso</text>
            </answer>
        </question> 
        <question codigo="q3Automata" type="truefalse">
           <questiontext format="html">
               <text>Un autómata finito determinista solo puede tener un estado final</text>
           </questiontext>
           <answer codigo="a1" >
                <text>verdadero</text>
            </answer>
            <answer codigo="a2">
                <text>falso</text>
            </answer>
        </question>
        <question codigo="q4Automata" type="multiple">
           <questiontext format="html">
               <text>Señala cual de las siguientes es una afirmación verdadera</text>
           </questiontext>
           <answer codigo="a1" >
                <text>Los AFD sirven para realizar el análisis sintáctico</text>
            </answer>
            <answer codigo="a2">
                <text>Los AFND solo reconocen lenguajes que contengan la palabra vacía</text>
            </answer>
            <answer codigo="a3">
                <text>Los AFD y los AFND son equivalentes</text>
            </answer>
        </question> 
        <question codigo="q5Automata" type="multiple">
           <questiontext format="html">
               <text>Señala cual de las siguientes es una afirmación falsa</text>
           </questiontext>
           <answer codigo="a1" >
                <text>Los AFD sirven para realizar el análisis léxico</text>
            </answer>
            <answer codigo="a2">
                <text>Los AFND solo reconocen lenguajes regulares</text>
            </answer>
            <answer codigo="a3">
                <text>los AFND tienen más potencia que los AFD</text>
            </answer>
        </question> 
